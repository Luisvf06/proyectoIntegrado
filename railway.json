{
    "$schema": "https://railway.app/railway.schema.json",
    "services": [
      {
        "name": "backend",
        "path": "backend",
        "build": {
          "builder": "NIXPACKS"
        },
        "deploy": {
          "runtime": "UNSPECIFIED",
          "numReplicas": 1,
          "sleepApplication": false,
          "restartPolicyType": "ON_FAILURE",
          "restartPolicyMaxRetries": 10
        }
      },
      {
        "name": "frontend",
        "path": "frontend",
        "build": {
          "builder": "NIXPACKS"
        },
        "deploy": {
          "runtime": "UNSPECIFIED",
          "numReplicas": 1,
          "sleepApplication": false,
          "restartPolicyType": "ON_FAILURE",
          "restartPolicyMaxRetries": 10
        }
      }
    ]
  }
  